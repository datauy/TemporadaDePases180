    <div class="container">
      <div class="row Aligner">
        <div class="col-xs-4 Aligner-item Aligner-item--bottom text-left">
          <a href="#"><img src="<%= asset_path('images/180CIENCIA.png') %>" alt="180 Ciencia"></a>
        </div>
        <div class="col-xs-4 Aligner-item Aligner-item--bottom text-center">
          <!-- <a href="https://twitter.com/share" class="twitter-share-button" data-via="DataUY" data-lang="es" data-related="portal180">Twittear</a>
          <script>!function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0],p=/^http:/.test(d.location)?'http':'https';if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src=p+'://platform.twitter.com/widgets.js';fjs.parentNode.insertBefore(js,fjs);}}(document, 'script', 'twitter-wjs');</script>

