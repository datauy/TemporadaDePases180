<%
    costos = mutualista['COSTO']
    metas = mutualista['METAS']
    espera = mutualista['TIEMPOS_ESPERA']
    satisfaccion = mutualista['SATISFACCION']
    personal = mutualista['PERSONAL']
%>
<div class="container">
    <div class="panel panel-danger Mb-0">
        <div class="panel-heading">
            <h4 class="panel-title"><%= posicion %> - <%= mutualista['NOMBRE'] %></h4>
        </div>
        <div class="panel-body">
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Costo:</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Medicamentos" style="width: 25%">$<%= costos['MEDICAMENTOS'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Consultas no urgentes" style="width: 25%">$<%= costos['CONSULTAS_NO_URGENTES'] %></div>
                        <div class="progress-bar progress-bar-success" data-toggle="tooltip" title="Consultas urgentes" style="width: 25%">$<%= costos['CONSULTAS_URGENTES'] %></div>
                        <div class="progress-bar progress-bar-info" data-toggle="tooltip" title="Estudios y laboratorios" style="width: 25%">$<%= costos['ESTUDIOS_Y_LABORATORIOS'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Metas asistenciales:</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Niños" style="width: <%= metas['NINOS'].to_f * 0.2 %>%"><%= metas['NINOS'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Embarazadas" style="width: <%= metas['EMBARAZADAS'].to_f * 0.2 %>%"><%= metas['EMBARAZADAS'] %></div>
                        <div class="progress-bar progress-bar-success" data-toggle="tooltip" title="Adolescentes" style="width: <%= metas['ADOLESCENTE'].to_f * 0.2 %>%"><%= metas['ADOLESCENTE'] %></div>
                        <div class="progress-bar progress-bar-info" data-toggle="tooltip" title="Adultos" style="width: <%= metas['ADULTO'].to_f * 0.2 %>%"><%= metas['ADULTO'] %></div>
                        <div class="progress-bar progress-bar-kore" data-toggle="tooltip" title="Adultos mayores" style="width: <%= metas['ADULTO_MAYOR'].to_f * 0.2 %>%"><%= metas['ADULTO_MAYOR'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Tiempos de espera:</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Medicina general" style="width: 25%"><%= espera['MEDICINA_GENERAL'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Pediatria" style="width: 25%"><%= espera['PEDIATRIA'] %></div>
                        <div class="progress-bar progress-bar-success" data-toggle="tooltip" title="Cirugía general" style="width: 25%"><%= espera['CIRUGIA_GENERAL'] %></div>
                        <div class="progress-bar progress-bar-info" data-toggle="tooltip" title="Ginecología" style="width: 25%"><%= espera['GINECOLOGIA'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Satisfación de usuarios:</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Quejas" style="width: <%= satisfaccion['QUEJAS'].to_f * 0.5 %>%"><%= satisfaccion['QUEJAS'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Derechos" style="width: <%= satisfaccion['DERECHOS'].to_f * 0.5 %>%"><%= satisfaccion['DERECHOS'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Personal disponible:</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Medicos" style="width: 20%"><%= personal['MEDICOS'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Ginecologos" style="width: 20%"><%= personal['GINECOLOGOS'] %></div>
                        <div class="progress-bar progress-bar-success" data-toggle="tooltip" title="Pediatras" style="width: 20%"><%= personal['PEDIATRAS'] %></div>
                        <div class="progress-bar progress-bar-info" data-toggle="tooltip" title="Enfermeros" style="width: 20%"><%= personal['ENFERMEROS'] %></div>
                        <div class="progress-bar progress-bar-kore" data-toggle="tooltip" title="Licenciados de Enfermeria" style="width: 20%"><%= personal['LICENFER'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Asignación de citas:</b></div>
                <div class="col-xs-8">
                <% if mutualista['CITAS_PERSONAL'] %>
                    <span class="label label-danger"><i class="glyphicon glyphicon-user"></i></span>
                <% end %>
                <% if mutualista['CITAS_TELEFONICA'] %>
                    <span class="label label-success"><i class="glyphicon glyphicon-earphone"></i></span>
                <% end %>
                <% if mutualista['CITAS_WEB'] %>
                    <span class="label label-warning"><i class="glyphicon glyphicon-globe"></i></span>
                <% end %>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-4 text-right"><b>Recordatorio consultas:</b></div>
                <div class="col-xs-8">
                <% if mutualista['RECORDATORIO_TELEFONO'] %>
                    <span class="label label-success"><i class="glyphicon glyphicon-earphone"></i></span>
                <% end %>
                <% if mutualista['RECORDATORIO_SMS'] %>
                    <span class="label label-warning"><i class="glyphicon glyphicon-phone"></i></span>
                <% end %>
                <% if mutualista['RECORDATORIO_CORREO'] %>
                    <span class="label label-info"><i class="glyphicon glyphicon-envelope"></i></span>
                <% end %>
                <% if !(mutualista['RECORDATORIO_TELEFONO'] || mutualista['RECORDATORIO_SMS'] || mutualista['RECORDATORIO_CORREO']) %>
                    <span class="label label-default"><i class="glyphicon glyphicon-remove"></i></span>
                <% end %>
                </div>
            </div>
        </div>
    </div>
</div>
