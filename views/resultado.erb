<%
    costos = mutualista['COSTO']
    costo_max = 1314
    metas = mutualista['METAS']
    espera = mutualista['TIEMPOS_ESPERA']
    espera_max = 26.24
    satisfaccion = mutualista['SATISFACCION']
    personal = mutualista['PERSONAL']
    personal_max = 129.1
%>

<div class="container">
    <div class="panel panel-danger Mb-0">
        <div class="panel-heading">
            <h4 class="panel-title"><%= posicion %> - <%= mutualista['NOMBRE'] %></h4>
        </div>
        <div class="panel-body">
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Costo promedio:</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Medicamentos" style="min-width: 5%; width: <%= (costos['MEDICAMENTOS'].to_f * 100)/costo_max %>%">$<%= costos['MEDICAMENTOS'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Consultas no urgentes" style="min-width: 5%; width: <%= (costos['CONSULTAS_NO_URGENTES'].to_f * 100)/costo_max %>%">$<%= costos['CONSULTAS_NO_URGENTES'] %></div>
                        <div class="progress-bar progress-bar-success" data-toggle="tooltip" title="Consultas urgentes" style="min-width: 5%; width: <%= (costos['CONSULTAS_URGENTES'].to_f * 100)/costo_max %>%">$<%= costos['CONSULTAS_URGENTES'] %></div>
                        <div class="progress-bar progress-bar-info" data-toggle="tooltip" title="Estudios y laboratorios" style="min-width: 5%; width: <%= (costos['ESTUDIOS_Y_LABORATORIOS'].to_f * 100)/costo_max %>%">$<%= costos['ESTUDIOS_Y_LABORATORIOS'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Metas asistenciales:</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Niños de un año controlados correctamente" style="min-width: 5%; width: <%= metas['NINOS'].to_f * 0.2 %>%"><%= metas['NINOS'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Embarazadas controladas correctamente" style="min-width: 5%; width: <%= metas['EMBARAZADAS'].to_f * 0.2 %>%"><%= metas['EMBARAZADAS'] %></div>
                        <div class="progress-bar progress-bar-success" data-toggle="tooltip" title="Adolescentes con consulta con su médico de referencia" style="min-width: 5%; width: <%= metas['ADOLESCENTE'].to_f * 0.2 %>%"><%= metas['ADOLESCENTE'] %></div>
                        <div class="progress-bar progress-bar-info" data-toggle="tooltip" title="Adultos con consulta con su médico de referencia" style="min-width: 5%; width: <%= metas['ADULTO'].to_f * 0.2 %>%"><%= metas['ADULTO'] %></div>
                        <div class="progress-bar progress-bar-kore" data-toggle="tooltip" title="Adultos mayores con consulta con su médico de referencia" style="min-width: 5%; width: <%= metas['ADULTO_MAYOR'].to_f * 0.2 %>%"><%= metas['ADULTO_MAYOR'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Tiempos de espera promedio (días):</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Medicina general" style="min-width: 5%; width: <%= ( espera['MEDICINA_GENERAL'].to_f * 100 )/espera_max %>%"><%= espera['MEDICINA_GENERAL'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Pediatria" style="min-width: 5%; width: <%= (espera['PEDIATRIA'].to_f * 100)/espera_max %>%"><%= espera['PEDIATRIA'] %></div>
                        <div class="progress-bar progress-bar-success" data-toggle="tooltip" title="Cirugía general" style=" min-width: 5%; width: <%= (espera['CIRUGIA_GENERAL'].to_f * 100)/espera_max %>%"><%= espera['CIRUGIA_GENERAL'] %></div>
                        <div class="progress-bar progress-bar-info" data-toggle="tooltip" title="Ginecología" style="min-width: 5%; width: <%= (espera['GINECOLOGIA'].to_f * 100)/espera_max %>%"><%= espera['GINECOLOGIA'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Derechos de usuarios:</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Entrega de información sobre quejas" style="min-width: 5%; width: <%= satisfaccion['QUEJAS'].to_f * 0.5 %>%"><%= satisfaccion['QUEJAS'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Entrega de información sobre derechos" style="min-width: 5%; width: <%= satisfaccion['DERECHOS'].to_f * 0.5 %>%"><%= satisfaccion['DERECHOS'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Personal disponible c/10.000 usuarios:</b></div>
                <div class="col-xs-7">
                    <div class="progress Mb-0">
                        <div class="progress-bar progress-bar-danger" data-toggle="tooltip" title="Medicos" style="min-width: 5%; width: <%= ( personal['MEDICOS'].to_f * 100 )/personal_max%>%"><%= personal['MEDICOS'] %></div>
                        <div class="progress-bar progress-bar-warning" data-toggle="tooltip" title="Ginecologos" style="min-width: 5%; width: <%= ( personal['GINECOLOGOS'].to_f * 100 )/personal_max%>%"><%= personal['GINECOLOGOS'] %></div>
                        <div class="progress-bar progress-bar-success" data-toggle="tooltip" title="Pediatras" style="min-width: 5%; width: <%= ( personal['PEDIATRAS'].to_f * 100 )/personal_max%>%"><%= personal['PEDIATRAS'] %></div>
                        <div class="progress-bar progress-bar-info" data-toggle="tooltip" title="Enfermeros" style="min-width: 5%; width: <%= ( personal['ENFERMEROS'].to_f * 100 )/personal_max%>%"><%= personal['ENFERMEROS'] %></div>
                        <div class="progress-bar progress-bar-kore" data-toggle="tooltip" title="Licenciados de Enfermeria" style="min-width: 5%; width: <%= ( personal['LICENFER'].to_f * 100 )/personal_max%>%"><%= personal['LICENFER'] %></div>
                    </div>
                </div>
                <div class="col-xs-1 text-center"></div>
            </div>
            <div class="row Mb-10">
                <div class="col-xs-4 text-right"><b>Asignación de citas:</b></div>
                <div class="col-xs-8">
                <% if mutualista['CITAS_PERSONAL'] %>
                    <span class="label label-danger" data-toggle="tooltip" title="Personalmente"><i class="glyphicon glyphicon-user"></i></span>
                <% end %>
                <% if mutualista['CITAS_TELEFONICA'] %>
                    <span class="label label-success" data-toggle="tooltip" title="Telefónicamente"><i class="glyphicon glyphicon-earphone"></i></span>
                <% end %>
                <% if mutualista['CITAS_WEB'] %>
                    <span class="label label-warning" data-toggle="tooltip" title="Vía web"><i class="glyphicon glyphicon-globe"></i></span>
                <% end %>
                </div>
            </div>
            <div class="row">
                <div class="col-xs-4 text-right"><b>Recordatorio consultas:</b></div>
                <div class="col-xs-8">
                <% if mutualista['RECORDATORIO_TELEFONO'] %>
                    <span class="label label-success" data-toggle="tooltip" title="Por teléfono"><i class="glyphicon glyphicon-earphone"></i></span>
                <% end %>
                <% if mutualista['RECORDATORIO_SMS'] %>
                    <span class="label label-warning" data-toggle="tooltip" title="Por SMS"><i class="glyphicon glyphicon-phone"></i></span>
                <% end %>
                <% if mutualista['RECORDATORIO_CORREO'] %>
                    <span class="label label-info" data-toggle="tooltip" title="Por correo electrónico"><i class="glyphicon glyphicon-envelope"></i></span>
                <% end %>
                <% if !(mutualista['RECORDATORIO_TELEFONO'] || mutualista['RECORDATORIO_SMS'] || mutualista['RECORDATORIO_CORREO']) %>
                    <span class="label label-default"><i class="glyphicon glyphicon-remove"></i></span>
                <% end %>
                </div>
            </div>
        </div>
    </div>
</div>
